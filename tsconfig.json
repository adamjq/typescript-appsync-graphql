{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "rootDir": "src",
    "outDir": "./dist",
    "baseUrl": "./",
    "typeRoots": [
      "node_modules/@types"
    ],

    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
//
//    "noUnusedLocals": true,
//    "noUnusedParameters": true,
    "types": [
      "node",
      "jest"
    ],
    "esModuleInterop": true,
    "inlineSourceMap": true
  }
}